<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context=".MainActivity">
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/btnParse"
            android:id="@+id/btnParse"
            android:onClick="onClickBtnParse"
            android:layout_above="@+id/btnBonLivraison" android:layout_centerHorizontal="true"
            android:layout_marginBottom="14dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" android:layout_below="@+id/btnParse" android:layout_centerHorizontal="true"
            android:layout_marginTop="12dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView"
            android:src="@drawable/titre"
            android:layout_alignParentTop="true" android:layout_centerHorizontal="true"
            android:contentDescription="@string/app_name" android:clickable="false"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView2" android:layout_centerHorizontal="true"
            android:src="@drawable/accueil" android:layout_below="@+id/imageView"
            android:layout_above="@+id/btnParse" android:contentDescription="@string/bonhomme"/>
</RelativeLayout>